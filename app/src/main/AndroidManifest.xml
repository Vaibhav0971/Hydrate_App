<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.version1">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".sub_menu" />
        <activity android:name=".menu" />
        <activity android:name=".profile" />
        <activity android:name=".main" />
        <activity android:name=".time" />
        <activity android:name=".weight" />
        <activity android:name=".gender" />
        <activity android:name=".welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".notification_broadcast"/>
        <receiver android:name=".snooze_broadcast"/>
        <!--
        <receiver android:name=".custom_broadcast"/>
        <receiver android:name=".recent_broadcast"/>
        -->
    </application>

</manifest>