        app:layout_constraintGuide_percent="0.49261" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h059"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.591133" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h068"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.68965" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h078"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.788177" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/h088"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.886699" />







    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/v005"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05000" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/v010"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.106666" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/v053"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.53333" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/v094"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.946666" />

    <ImageView
        android:id="@+id/drop_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="0.6"
        app:layout_constraintStart_toStartOf="@+id/v053"
        app:layout_constraintTop_toTopOf="@+id/h013"
        app:srcCompat="@drawable/bg_drop" />

    <ImageView
        android:id="@+id/back_button"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:onClick="back_button"
        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h003"
        app:srcCompat="@drawable/back_button" />

    <TextView
        android:id="@+id/settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/berlin_sans_fb"
        android:text="Settings"
        android:textColor="@color/white"
        android:textSize="52sp"
        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h008" />


    <ImageView
        android:id="@+id/tips_bg"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:onClick="tips_bg"
        app:layout_constraintEnd_toStartOf="@+id/v094"
        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h019"
        app:srcCompat="@drawable/white_background" />

    <ImageView
        android:id="@+id/general_bg"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:onClick="general_bg"
