        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h029"
        app:srcCompat="@drawable/white_background" />

    <ImageView
        android:id="@+id/personal_info_bg"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:onClick="personal_info_bg"
        app:layout_constraintEnd_toStartOf="@+id/v094"
        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h039"
        app:srcCompat="@drawable/white_background" />

    <ImageView
        android:id="@+id/remainder_schedule_bg"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:onClick="remainder_schedule_bg"
        app:layout_constraintEnd_toStartOf="@+id/v094"
        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h049"
        app:srcCompat="@drawable/white_background" />

    <ImageView
        android:id="@+id/remainder_sound_bg"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:onClick="remainder_sound_bg"
        app:layout_constraintEnd_toStartOf="@+id/v094"
        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h059"
        app:srcCompat="@drawable/white_background" />

    <ImageView
        android:id="@+id/remainder_mode_bg"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:onClick="remainder_mode_bg"
        app:layout_constraintEnd_toStartOf="@+id/v094"
        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h068"
        app:srcCompat="@drawable/white_background" />

    <ImageView
        android:id="@+id/further_remainder_bg"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:onClick="further_remainder_bg"
        app:layout_constraintEnd_toStartOf="@+id/v094"
        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h078"
        app:srcCompat="@drawable/white_background" />

    <ImageView
        android:id="@+id/reset_data_bg"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:onClick="reset_data_bg"
        app:layout_constraintEnd_toStartOf="@+id/v094"
        app:layout_constraintStart_toStartOf="@+id/v005"
        app:layout_constraintTop_toTopOf="@+id/h088"
        app:srcCompat="@drawable/white_background" />


    <TextView
        android:id="@+id/tips"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_regular"
        android:onClick="tips"
        android:text="Tips"
        android:textColor="#000000"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/tips_bg"
        app:layout_constraintStart_toStartOf="@+id/v010"
        app:layout_constraintTop_toTopOf="@+id/tips_bg" />

    <TextView
        android:id="@+id/general"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_regular"
        android:onClick="general"
        android:text="General"
        android:textColor="#000000"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/general_bg"
        app:layout_constraintStart_toStartOf="@+id/v010"
        app:layout_constraintTop_toTopOf="@+id/general_bg" />

    <TextView
        android:id="@+id/personal_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_regular"
        android:onClick="personal_info"
        android:text="Personal info"
        android:textColor="#000000"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/personal_info_bg"
        app:layout_constraintStart_toStartOf="@+id/v010"
        app:layout_constraintTop_toTopOf="@+id/personal_info_bg" />

    <TextView
        android:id="@+id/remainder_schedule"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_regular"
        android:onClick="remainder_schedule"
        android:text="Remainder schedule"
        android:textColor="#000000"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/remainder_schedule_bg"
        app:layout_constraintStart_toStartOf="@+id/v010"
        app:layout_constraintTop_toTopOf="@+id/remainder_schedule_bg" />

    <TextView
        android:id="@+id/remainder_sound"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_regular"
        android:onClick="remainder_sound"
        android:text="Remainder sound"
        android:textColor="#000000"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/remainder_sound_bg"
        app:layout_constraintStart_toStartOf="@+id/v010"
        app:layout_constraintTop_toTopOf="@+id/remainder_sound_bg" />

    <TextView
        android:id="@+id/remainder_mode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato_regular"
        android:onClick="remainder_mode"
        android:text="Remainder mode"
        android:textColor="#000000"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/remainder_mode_bg"
        app:layout_constraintStart_toStartOf="@+id/v010"
